{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/chat",
        "dest": "server.js",
        "methods": ["POST"]
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }